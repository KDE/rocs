<?xml version="1.0"?>
<object>
<name>Edge</name>
<id>Edge</id>
<componentType>Edge</componentType>
<description>
<para>
An edge is the connection of two nodes. Edges can be either directed or undirected, depending on their type.
</para>
</description>
<syntax>
</syntax>
<properties>
<property>
    <name>type</name>
    <description>
        <para>Read and set the identifier of the edge's type. If setting, the edge type identifier must exist.</para>
    </description>
    <type>string</type>
</property>
<property>
    <name>weight</name>
    <description>
        <para>Read and set the weight of the edge.</para>
    </description>
    <type>real</type>
</property>
</properties>
<methods>
<method>
    <name>property(identifier)</name>
    <description>
        <para>Return the value of the property as currently set.</para>
    </description>
    <returnType>variant</returnType>
    <parameters>
        <parameter>
            <name>identifier</name>
            <type>string</type>
            <info>Identifier of the property</info>
        </parameter>
    </parameters>
</method>
<method>
    <name>setProperty(identifier, value)</name>
    <description>
        <para>Set the property selected by identifier to the given value.</para>
    </description>
    <returnType>void</returnType>
    <parameters>
        <parameter>
            <name>identifier</name>
            <type>string</type>
            <info>Identifier the property</info>
        </parameter>
        <parameter>
            <name>value</name>
            <type>variant</type>
            <info>The value to be set for the property (JavaScript val).</info>
        </parameter>
    </parameters>
</method>
<method>
    <name>from()</name>
    <description>
        <para>Return the origin node of the edge.</para>
    </description>
    <returnType>Node</returnType>
    <parameters>
    </parameters>
</method>
<method>
    <name>to()</name>
    <description>
        <para>Return the target node of the edge.</para>
    </description>
    <returnType>Node</returnType>
    <parameters>
    </parameters>
</method>
<method>
    <name>directed()</name>
    <description>
        <para>Returns true if the edge's type is directed, otherwise false.</para>
    </description>
    <returnType>bool</returnType>
    <parameters>
    </parameters>
</method>
</methods>
</object>
